{
    "name": "sperelson/laravel-single-redirect",
    "description": "Adds ability to reduce multiple redirects down to one",
    "keywords": ["laravel", "redirect", "reduce", "middleware"],
    "license": "MIT",
    "authors": [
        {
            "name": "SPerelson",
            "homepage": "https://perelson.xyz"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "laravel/framework": "^9|^10"
    },
    "autoload": {
        "psr-4": {
            "Perelson\\SingleRedirect\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Perelson\\SingleRedirect\\SingleRedirectServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
